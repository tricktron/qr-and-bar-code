{
  "name": "qr-and-barcode",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "devDependencies": {
    "@fitbit/sdk": "~4.3.2",
    "@fitbit/sdk-cli": "^1.7.3",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "fitbit-sdk-types": "^7.0.0",
    "jest": "^27.2.1",
    "rollup-plugin-purs": "^1.0.38",
    "sharp": "^0.29.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  },
  "fitbit": {
    "appUUID": "5b1d78ff-a238-4024-8dc4-a2171d134a34",
    "appType": "app",
    "appDisplayName": "QR and Barcode",
    "iconFile": "resources/icon.png",
    "wipeColor": "#ffffff",
    "requestedPermissions": [],
    "buildTargets": [
      "higgs",
      "meson",
      "gemini",
      "mira"
    ],
    "i18n": {},
    "defaultLanguage": "en-US"
  },
  "scripts": {
    "build": "fitbit-build",
    "debug": "fitbit",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "roots": [
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "((\\.|/)(test|spec))\\.tsx?$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/*.ts"
    ],
    "moduleNameMapper": {
      "mocks": "app/test/mocks.ts"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@fitbit/image-codec-txi": "^1.1.5",
    "@zxing/library": "^0.18.6",
    "jsqr": "^1.4.0",
    "qrcode-generator": "^1.4.4"
  }
}
